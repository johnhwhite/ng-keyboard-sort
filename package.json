{
  "name": "ng-keyboard-sort",
  "description": "Angular keyboard sort directive",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhwhite/ng-keyboard-sort.git",
    "directory": "libs/ng-keyboard-sort"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng s e2e",
    "build": "ng b ng-keyboard-sort && ng b e2e",
    "build:package": "ng b ng-keyboard-sort -c production",
    "watch": "ng b ng-keyboard-sort --watch -c development",
    "test": "ng test ng-keyboard-sort --configuration=ci",
    "test:e2e": "ng test e2e --configuration=ci",
    "test:all": "ng test --configuration=ci --no-watch",
    "pree2e": "playwright install --with-deps",
    "e2e": "ng e2e -c ci",
    "lint": "ng lint ng-keyboard-sort",
    "lint:e2e": "ng lint e2e",
    "lint:all": "ng lint",
    "// prepare": "Set up git hooks. Does not run in a CI environment.",
    "prepare": "is-ci || husky",
    "// postinstall": "Postinstall script to update peer dependencies. Does not run in a CI environment.",
    "postinstall": "is-ci || node scripts/postinstall.mjs",
    "commitlint": "commitlint --edit",
    "validate-commit-msg": "commitlint --strict"
  },
  "dependencies": {
    "@angular/animations": "20.3.5",
    "@angular/cdk": "20.2.9",
    "@angular/common": "20.3.5",
    "@angular/core": "20.3.5",
    "@angular/forms": "20.3.5",
    "@angular/platform-browser": "20.3.5",
    "@angular/platform-browser-dynamic": "20.3.5",
    "@angular/router": "20.3.5",
    "rxjs": "~7.8.2",
    "semver": "^7.7.2",
    "tslib": "^2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.5",
    "@angular-devkit/core": "20.3.5",
    "@angular-devkit/schematics": "20.3.5",
    "@angular-eslint/builder": "^20.4.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/build": "20.3.5",
    "@angular/cli": "~20.3.5",
    "@angular/compiler": "20.3.5",
    "@angular/compiler-cli": "20.3.5",
    "@angular/language-service": "20.3.5",
    "@angular/localize": "20.3.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-angular": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@types/jasmine": "~5.1.9",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@typescript-eslint/types": "^8.43.0",
    "@typescript-eslint/typescript-estree": "^8.43.0",
    "@typescript-eslint/utils": "^8.43.0",
    "angular-eslint": "^20.4.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-webkit-launcher": "^2.6.0",
    "ng-packagr": "20.3.0",
    "playwright-ng-schematics": "^2.1.0",
    "playwright-webkit": "^1.55.0",
    "postcss": "^8.5.6",
    "postcss-import": "~16.1.1",
    "postcss-preset-env": "~10.3.1",
    "postcss-url": "~10.1.3",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5"
  }
}
