{
  "name": "ng-keyboard-sort",
  "description": "Angular keyboard sort directive",
  "version": "2.0.4",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhwhite/ng-keyboard-sort.git",
    "directory": "libs/ng-keyboard-sort"
  },
  "scripts": {
    "ng": "ng",
    "start": "nx s e2e",
    "build": "nx run-many --target=build --all",
    "build:package": "nx run ng-keyboard-sort:build:production",
    "watch": "nx build ng-keyboard-sort --watch --configuration development",
    "test": "nx test ng-keyboard-sort --browsers=ChromeHeadless --code-coverage --watch=false",
    "test:e2e": "nx test e2e --browsers=ChromeHeadless --code-coverage --watch=false",
    "test:all": "nx affected:test --all --browsers=ChromeHeadless --code-coverage --watch=false",
    "e2e": "nx run-many --target=e2e --all",
    "lint": "nx run-many --target=lint --all --parallel",
    "commit": "cz",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/core": "^15.2.5",
