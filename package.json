{
  "name": "ng-keyboard-sort",
  "description": "Angular keyboard sort directive",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhwhite/ng-keyboard-sort.git",
    "directory": "libs/ng-keyboard-sort"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng s e2e",
    "build": "ng b ng-keyboard-sort && ng b e2e",
    "build:package": "ng b ng-keyboard-sort -c production",
    "watch": "ng b ng-keyboard-sort --watch -c development",
    "test": "ng test ng-keyboard-sort --configuration=ci",
    "test:e2e": "ng test e2e --configuration=ci",
    "test:all": "ng test --configuration=ci --no-watch",
    "pree2e": "playwright install --with-deps",
    "e2e": "playwright test -c apps/e2e/playwright.config.ts --browser=all",
    "lint": "ng lint ng-keyboard-sort",
    "lint:e2e": "ng lint e2e",
    "lint:all": "ng lint",
    "commit": "cz",
    "prepare": "is-ci || husky",
    "postinstall": "node scripts/postinstall.mjs",
    "commitlint": "commitlint --edit",
    "validate-commit-msg": "commitlint --strict"
  },
  "dependencies": {
    "@angular/animations": "19.2.4",
    "@angular/cdk": "19.2.7",
    "@angular/common": "19.2.4",
    "@angular/core": "19.2.4",
    "@angular/forms": "19.2.4",
    "@angular/platform-browser": "19.2.4",
    "@angular/platform-browser-dynamic": "19.2.4",
    "@angular/router": "19.2.4",
    "rxjs": "~7.8.2",
    "semver": "^7.7.1",
    "tslib": "^2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.5",
    "@angular-devkit/core": "19.2.5",
    "@angular-devkit/schematics": "19.2.5",
    "@angular-eslint/builder": "^19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/build": "19.2.5",
    "@angular/cli": "~19.2.5",
    "@angular/compiler": "19.2.4",
    "@angular/compiler-cli": "19.2.4",
    "@angular/language-service": "19.2.4",
    "@angular/localize": "19.2.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-angular": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.51.1",
    "@types/jasmine": "~5.1.7",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@typescript-eslint/typescript-estree": "^8.28.0",
    "@typescript-eslint/utils": "^8.28.0",
    "angular-eslint": "^19.3.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-webkit-launcher": "^2.6.0",
    "ng-packagr": "19.2.0",
    "playwright-webkit": "^1.51.1",
    "postcss": "^8.5.3",
    "postcss-import": "~16.1.0",
    "postcss-preset-env": "~10.1.5",
    "postcss-url": "~10.1.3",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.28.0",
    "uuid": "^11.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
