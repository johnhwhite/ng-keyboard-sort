{
  "name": "ng-keyboard-sort",
  "description": "Angular keyboard sort directive",
  "version": "3.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhwhite/ng-keyboard-sort.git",
    "directory": "libs/ng-keyboard-sort"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng s e2e",
    "build": "ng b ng-keyboard-sort && ng b e2e",
    "build:package": "ng b ng-keyboard-sort -c production",
    "watch": "ng b ng-keyboard-sort --watch -c development",
    "// test": "karma-webkit-launcher requires CI=false and alters its behavior based on ci-info",
    "test": "CI=false ng test ng-keyboard-sort --configuration=ci",
    "test:e2e": "CI=false ng test e2e --configuration=ci",
    "test:all": "npm run test && npm run test:e2e",
    "pree2e": "playwright install --with-deps",
    "e2e": "playwright test -c apps/e2e/playwright.config.ts --browser=all",
    "lint": "ng lint ng-keyboard-sort",
    "lint:e2e": "ng lint ng-keyboard-sort",
    "lint:all": "npm run lint && npm run lint:e2e",
    "commit": "cz",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@angular/animations": "16.1.3",
    "@angular/cdk": "16.1.3",
    "@angular/common": "16.1.3",
    "@angular/core": "16.1.3",
    "@angular/forms": "16.1.3",
    "@angular/platform-browser": "16.1.3",
    "@angular/platform-browser-dynamic": "16.1.3",
    "@angular/router": "16.1.3",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.3",
    "@angular-devkit/core": "16.1.3",
    "@angular-devkit/schematics": "16.1.3",
    "@angular-eslint/builder": "^16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.3",
    "@angular/compiler": "16.1.3",
    "@angular/compiler-cli": "16.1.3",
    "@angular/language-service": "16.1.3",
    "@angular/localize": "16.1.3",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-angular": "^17.6.6",
    "@playwright/test": "^1.35.1",
    "@schematics/angular": "16.1.3",
    "@types/jasmine": "~4.3.5",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@typescript-eslint/typescript-estree": "^5.61.0",
    "commitizen": "^4.3.0",
    "eslint": "~8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-webkit-launcher": "^2.1.0",
    "ng-packagr": "16.1.0",
    "playwright-webkit": "^1.35.1",
    "postcss": "^8.4.24",
    "postcss-import": "~15.1.0",
    "postcss-preset-env": "~8.5.1",
    "postcss-url": "~10.1.3",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "typescript": "5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
