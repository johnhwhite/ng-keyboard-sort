{
  "name": "ng-keyboard-sort",
  "description": "Angular keyboard sort directive",
  "version": "3.1.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhwhite/ng-keyboard-sort.git",
    "directory": "libs/ng-keyboard-sort"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng s e2e",
    "build": "ng b ng-keyboard-sort && ng b e2e",
    "build:package": "ng b ng-keyboard-sort -c production",
    "watch": "ng b ng-keyboard-sort --watch -c development",
    "test": "ng test ng-keyboard-sort --configuration=ci",
    "test:e2e": "ng test e2e --configuration=ci",
    "test:all": "npm run test && npm run test:e2e",
    "pree2e": "playwright install --with-deps",
    "e2e": "playwright test -c apps/e2e/playwright.config.ts --browser=all",
    "lint": "ng lint ng-keyboard-sort",
    "lint:e2e": "ng lint ng-keyboard-sort",
    "lint:all": "npm run lint && npm run lint:e2e",
    "commit": "cz",
    "prepare": "is-ci || husky install",
    "commitlint": "commitlint --edit",
    "validate-commit-msg": "commitlint --strict"
  },
  "dependencies": {
    "@angular/animations": "16.2.0",
    "@angular/cdk": "16.2.0",
    "@angular/common": "16.2.0",
    "@angular/core": "16.2.0",
    "@angular/forms": "16.2.0",
    "@angular/platform-browser": "16.2.0",
    "@angular/platform-browser-dynamic": "16.2.0",
    "@angular/router": "16.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.0",
    "@angular-devkit/core": "16.2.0",
    "@angular-devkit/schematics": "16.2.0",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler": "16.2.0",
    "@angular/compiler-cli": "16.2.0",
    "@angular/language-service": "16.2.0",
    "@angular/localize": "16.2.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-angular": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@playwright/test": "^1.37.0",
    "@schematics/angular": "16.2.0",
    "@types/jasmine": "~4.3.5",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@typescript-eslint/typescript-estree": "^6.3.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-webkit-launcher": "^2.1.0",
    "ng-packagr": "16.2.0",
    "playwright-webkit": "^1.37.0",
    "postcss": "^8.4.27",
    "postcss-import": "~15.1.0",
    "postcss-preset-env": "~9.1.1",
    "postcss-url": "~10.1.3",
    "prettier": "^3.0.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "5.1.6",
    "uuid": "^9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
